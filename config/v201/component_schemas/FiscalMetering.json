{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Schema for FiscalMetering",
  "type": "object",
  "properties": {
    "EnergyExport": {
      "characteristics": {
        "unit": "Wh",
        "supportsMonitoring": true,
        "dataType": "decimal"
      },
      "attributes": [
        {
          "type": "Actual"
        }
      ],
      "description": " kWh,Total energy transferred: e.g.  from EV during (ongoing or terminated) charging session (in wH by default)",
      "type": "number"
    },
    "EnergyExportRegister": {
      "characteristics": {
        "unit": "Wh",
        "supportsMonitoring": true,
        "dataType": "decimal"
      },
      "attributes": [
        {
          "type": "Actual"
        }
      ],
      "description": " kWh,Cumulative export kWh register value, such as from a (certified) fiscal energy meter.",
      "type": "number"
    },
    "EnergyImport": {
      "characteristics": {
        "unit": "Wh",
        "supportsMonitoring": true,
        "dataType": "decimal"
      },
      "attributes": [
        {
          "type": "Actual"
        }
      ],
      "description": " kWh,Total energy transferred.",
      "type": "number"
    },
    "EnergyImportRegister": {
      "characteristics": {
        "unit": "Wh",
        "supportsMonitoring": true,
        "dataType": "decimal"
      },
      "attributes": [
        {
          "type": "Actual"
        }
      ],
      "description": " kWh,Cumulative export kWh register value, such as from a (certified) fiscal energy meter.",
      "type": "number"
    }
  },
  "required": []
}